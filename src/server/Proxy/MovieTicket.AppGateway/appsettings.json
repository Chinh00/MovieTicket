{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "clientapi": {
        "ClusterId": "client-api",
        "Match": {
          "Path": "/client-api/{**remainder}"
        },
        "Transforms": [
          {"PathRemovePrefix": "/client-api"},
          {"PathPrefix": "/api"}
        ]
      },
      "clientidentity": {
        "ClusterId": "client-identity",
        "Match": {
          "Path": "/client-identity/{**remainder}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/client-identity"
          },
          {
            "PathPrefix": "/"
          }
        ]
      },
      "adminapi": {
        "ClusterId": "admin-api",
        "Match": {
          "Path": "/admin-api/{**remainder}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/admin-api"
          },
          {
            "PathPrefix": "/api"
          }
        ]
      },
      "adminpublic": {
        "ClusterId": "admin-api",
        "Match": {
          "Path": "/admin-api/image/{**remainder}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/admin-api/image"
          },
          {
            "PathPrefix": ""
          }
        ]
      },
      "adminidentity": {
        "ClusterId": "admin-identity",
        "Match": {
          "Path": "/admin-identity/{**remainder}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/admin-identity"
          },
          {
            "PathPrefix": "/"
          }
        ]
      },
      "adminui": {
        "ClusterId": "admin-api",
        "Match": {
          "Path": "/admin/{**remainder}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/admin"
          },
          {
            "PathPrefix": "/"
          }
        ]
      }
    },
    "Clusters": {
      "client-api": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5006"
          }
        }
      },
      "client-identity": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5001"
          }
        }
      },
      "admin-api": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5002"
          }
        }
      },
      "admin-identity": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5003"
          }
        }
      }
    }
  }
}
